{
  "hierarchy": {
    "levels": [
      "Auxiliar",
      "Assistente",
      "Analista",
      "Especialista",
      "Coordenador",
      "Gerente",
      "Diretor"
    ],
    "career_type": {
      "no_team": "Professional",
      "has_team": "Management"
    },
    "rules": [
      {
        "if_input_contains": ["auxiliar", "assistente"],
        "must_not_return": ["coordenador", "gerente", "diretor"],
        "prefer_levels": ["analista"]
      },
      {
        "if_input_contains": ["analista"],
        "must_not_return": ["gerente", "diretor", "coordenador"],
        "prefer_levels": ["assistente", "especialista"]
      },
      {
        "if_input_contains": ["especialista"],
        "must_not_return": ["gerente", "diretor"],
        "prefer_levels": ["analista", "coordenador"]
      },
      {
        "if_input_contains": ["coordenador"],
        "must_not_return": ["gerente", "diretor"],
        "prefer_levels": ["especialista", "analista"]
      },
      {
        "if_input_contains": ["supervisor"],
        "must_not_return": ["gerente", "diretor"],
        "prefer_levels": ["coordenador", "especialista"]
      },
      {
        "if_input_contains": ["gerente"],
        "must_not_return": ["auxiliar", "assistente", "analista"],
        "prefer_levels": ["coordenador", "especialista"]
      },
      {
        "if_input_contains": ["diretor", "head", "líder executivo"],
        "must_not_return": ["auxiliar", "assistente", "analista", "coordenador"],
        "prefer_levels": ["gerente"]
      }
    ]
  },
  "weight_factors": {
    "scope": {
      "local": 1.0,
      "regional": 1.2,
      "global": 1.5
    },
    "team_size": {
      "none": 1.0,
      "small": 1.2,
      "medium": 1.4,
      "large": 1.6
    },
    "functions": {
      "single": 1.0,
      "multiple": 1.3
    }
  },
  "semantic_synonyms": {
    "auxiliar": ["assistant", "junior", "entry level"],
    "assistente": ["assistant", "support", "clerk"],
    "analista": ["analyst", "associate", "consultant"],
    "especialista": ["specialist", "expert", "advisor"],
    "coordenador": ["coordinator", "lead", "supervisor"],
    "supervisor": ["foreman", "team leader", "coordinator"],
    "gerente": ["manager", "head", "leader"],
    "diretor": ["director", "executive director", "vp", "head of"],
    "gestão": ["management", "leadership", "oversight"],
    "profissional": ["individual contributor", "technical role", "professional"],
    "projeto": ["project", "initiative", "program"],
    "equipe": ["team", "staff", "employees", "reporting line"],
    "subordinado": ["direct report", "staff", "team member"],
    "abrangência": ["scope", "coverage", "span of control"],
    "função": ["function", "role", "responsibility"]
  },
  "business_logic": {
    "career_rules": [
      {
        "description": "Se não possuir equipe, a carreira é Professional.",
        "condition": "team_size == 'none'",
        "career_type": "Professional"
      },
      {
        "description": "Se possuir equipe, a carreira é Management, mas apenas acima de Coordenador.",
        "condition": "team_size != 'none' and level in ['Coordenador', 'Gerente', 'Diretor']",
        "career_type": "Management"
      },
      {
        "description": "Se o cargo for abaixo de Coordenador e tiver equipe, reclassificar como Profissional.",
        "condition": "team_size != 'none' and level in ['Auxiliar','Assistente','Analista']",
        "career_type": "Professional"
      }
    ],
    "hierarchy_weight_rules": [
      {
        "description": "Maior abrangência aumenta o peso do cargo.",
        "factor": "scope",
        "mapping": {
          "local": 1.0,
          "regional": 1.2,
          "global": 1.5
        }
      },
      {
        "description": "Maior número de subordinados aumenta o peso do cargo.",
        "factor": "team_size",
        "mapping": {
          "none": 1.0,
          "small": 1.2,
          "medium": 1.4,
          "large": 1.6
        }
      },
      {
        "description": "Múltiplas áreas ou funções elevam o peso.",
        "factor": "functions",
        "mapping": {
          "single": 1.0,
          "multiple": 1.3
        }
      }
    ]
  },
  "semantic_boost": {
    "leadership_terms": [
      "liderança",
      "coordenação",
      "gestão",
      "supervisão",
      "planejamento estratégico"
    ],
    "technical_terms": [
      "execução",
      "técnico",
      "especialista",
      "analítico",
      "operacional"
    ],
    "scope_terms": {
      "global": ["regional", "global", "corporativo", "matriz"],
      "regional": ["regional", "cluster", "multi-country"],
      "local": ["local", "planta", "fábrica", "filial"]
    }
  },
  "matching_parameters": {
    "semantic_similarity_weight": 0.75,
    "hierarchy_coherence_weight": 0.15,
    "scope_and_team_weight": 0.10
  }
}
